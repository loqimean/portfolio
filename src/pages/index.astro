---
import Layout from '../layouts/Layout.astro';
import MainPageContent from '../components/MainPageContent.astro';
import { getTranslations, DEFAULT_LANGUAGE } from '../i18n/translations';

const t = getTranslations(DEFAULT_LANGUAGE);
---

<Layout title={`${t.name} - Portfolio`} lang={DEFAULT_LANGUAGE} metaDescription={t.index.metaDescription}>
  <MainPageContent t={t} />
</Layout>
